{
    "compilerOptions": {
      "target": "es2017",
      "module": "NodeNext",
      "rootDir": "./src",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false,
      "skipLibCheck": true,
      "outDir": "./dist",
      "moduleResolution": "NodeNext",
      "typeRoots": ["./node_modules/@types", "./src/types"]
    }
  }
  